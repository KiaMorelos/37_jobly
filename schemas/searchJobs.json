{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [],
    "additionalProperties": false,
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "default": "",
            "minLength": 1,
            "examples": [
                "Advertising account executive"
            ]
        },
        "minSalary": {
            "title": "The minSalary Schema",
            "type": "integer",
            "mininum": 0,
            "examples": [
                300000
            ]
        },
        "hasEquity": {
            "title": "The hasEquity Schema",
            "type": "boolean",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "title": "Advertising account executive",
        "minSalary": 300000,
        "hasEquity": true
    }]
}